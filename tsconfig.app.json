{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "outDir": "./out-tsc/app",
        "types": [],
        "paths": {
            "@env": [
                "src/environments"
            ],
            "@env/*": [
                "src/environments/*"
            ],
            "@core": [
                "src/app/core"
            ],
            "@core/*": [
                "src/app/core/*"
            ],
            "@routes": [
                "src/app/routes"
            ],
            "@routes/*": [
                "src/app/routes/*"
            ],
            "@shared": [
                "src/app/shared"
            ],
            "@shared/*": [
                "src/app/shared/*"
            ]
        }
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/test.ts",
        "src/**/*.spec.ts",
        "src/**/*.avoid.ts",
        "src/**/*.0.ts",
        "src/**/*.1.ts",
        "src/**/*.1b.ts",
        "src/**/*.2.ts",
        "src/**/*.3.ts",
        "src/**/*.4.ts",
        "src/**/*.5.ts",
        "src/**/*.6.ts",
        "src/**/*.7.ts"
    ]
}
